#!/bin/sh

start()
{
  local self="`realpath "$0"`"
  local base="`dirname -- "$self"`"

  source "$base/../env" &&
  merge_environment /srv || return 255

  exec "$PACKAGE_ROOT/sbin/sshd" -D 2>&1 \
    | /boot/timestamp \
    >> "$PACKAGE_STORAGE/openssh/logs/startup.log"
}

main()
{
  start
  return "$?"
}

main
exit "$?"

